{
  "name": "image_server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www --watch .",
    "production": "pm2 start bin/www --name \"ImageServer\" --watch ",
    "forever": "forever start ./bin/www",
    "forevers": "forever start -a --uid --watch \"ImageServer\" -c \"node ./bin/www\" ./",
    "stop": "pm2 delete ImageServer ",
    "stops": "forever stop \"ImageServer\" ",
    "stops_": "forever stop ./bin/www",
    "test": "NODE_ENV=test nodemon --exec 'mocha --recursive -R min'"
  },
  "dependencies": {
    "amqplib": "^0.5.5",
    "body-parser": "~1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cookie-session": "^1.4.0",
    "data-uri-to-buffer": "^3.0.1",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "~4.17.1",
    "fs": "0.0.1-security",
    "google-libphonenumber": "^3.2.11",
    "handlebars": "^4.7.6",
    "helmet": "^3.21.1",
    "image-to-data-uri": "^1.1.0",
    "ipfs-api": "^17.3.0",
    "mariadb": "^2.4.2",
    "md5": "^2.3.0",
    "mocha": "^6.2.2",
    "mongoose": "^5.7.7",
    "morgan": "~1.9.1",
    "multer": "^1.3.0",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.11",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "request-promise-native": "^1.0.9",
    "sequelize": "^6.3.4",
    "supertest": "^4.0.2",
    "swagger-jsdoc": "^3.4.0",
    "swagger-ui-express": "^4.1.2",
    "twing": "^4.0.2"
  }
}
